{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx tailwindcss -i ./src/index.css -o ./src/output.css && bun --hot src/index.tsx",
    "build": "bunx tailwindcss -i ./src/index.css -o ./src/output.css && bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/index.tsx",
    "css:watch": "bunx tailwindcss -i ./src/index.css -o ./src/output.css --watch"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.89.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "lucide-react": "^0.544.0",
    "postcss": "^8.5.6",
    "react": "^19",
    "react-dom": "^19",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.2.1",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  }
}
